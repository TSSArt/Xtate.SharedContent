<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<LangVersion>8</LangVersion>
		<TargetFrameworks>net8.0;netstandard2.0;net462</TargetFrameworks>
		<Nullable>enable</Nullable>
		<RepositoryUrl>https://github.com/TSSArt/Xtate.SharedContent</RepositoryUrl>
		<PackageId>Xtate.SharedContent</PackageId>
		<NuspecFile>Xtate.SharedContent.nuspec</NuspecFile>
		<NuspecBasePath>PackageContent</NuspecBasePath>
		<NoPackageAnalysis>true</NoPackageAnalysis>
	</PropertyGroup>

	<ItemGroup Condition="'$(TargetFramework)'=='net462'">
		<PackageReference Include="System.ValueTuple" Version="4.5.0" />
	</ItemGroup>
</Project>